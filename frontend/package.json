{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite",
    "generate": "./generate.bash",
    "frontend": "bun msw init public --save && vite --mode frontend",
    "mock": "bun msw init public --save && vite --mode mock",
    "development": "tsc -b && vite build",
    "build": "tsc -b && vite build",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint src --fix --color"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.71.5",
    "@tanstack/react-router": "^1.114.29",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint-plugin-react-dom": "^1.52.2",
    "eslint-plugin-react-x": "^1.52.2",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lucide-react": "^0.536.0",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.14",
    "ts-morph": "^26.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tanstack/react-router-devtools": "^1.114.31",
    "@tanstack/router-plugin": "^1.114.31",
    "@types/bun": "^1.2.18",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^7.0.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
