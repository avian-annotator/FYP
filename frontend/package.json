{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite",
    "frontend": "bun msw init public --save && vite --mode frontend",
    "mock": "bun msw init public --save && vite --mode mock",
    "development": "tsc -b && vite build --mode development",
    "production": "tsc -b && vite build --mode production",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint src --fix --color"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.71.5",
    "@tanstack/react-router": "^1.114.29",
    "axios": "^1.8.4",
    "eslint-plugin-react-dom": "^1.52.2",
    "eslint-plugin-react-x": "^1.52.2",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tanstack/react-router-devtools": "^1.114.31",
    "@tanstack/router-plugin": "^1.114.31",
    "@types/bun": "^1.2.18",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}